=Global Active Power(kilowatts)\n  #y=Frequency\n  # Type of graph: Histogram\n  \n  print(\"Creating plot1\")\n  png(file = outputfilename) ## Open the PNG Device\n  hist(mydata$Global_active_power, ylab = \"Frequency\", xlab = \"Global Active Power (kilowatts)\", main=\"Global Active Power\", col=\"Red\")\n  dev.off()\n}\n\ncreatemyplot2 <- function(mydata, outputfilename) {\n  print(\"Creating plot2\")\n  #x=Global Active Power(kilowatts)\n  #y=Days (Thu, Fri, Sat)\n  # Type of graph: line\n  \n  \n  \n  png(file = outputfilename) ## Open the PNG Device\n  \n  plot(mydata$datetime, mydata$Global_active_power, type=\"n\", ylab = \"Global Active Power (kilowatts)\", xlab=NULL)\n  lines(mydata$datetime, mydata$Global_active_power, type=\"l\")\n  \n  dev.off()\n}\n\n\n\nproject <- function(){\n\tfulldatafile = \"./data/household_power_consumption.txt\"\n\t#testdimensions(fulldatafile)\n\t\n\t# Read & filter the data\n\tmydata <- subsetdata(fulldatafile)\n\n\tcreatemyplot1(mydata, \"plot1.png\")\n\tcreatemyplot2(mydata, \"plot2.png\")\n\t#createmyplot3(mydata, \"plot3.png\")\n\t#createmyplot4(mydata, \"plot4.png\")\n\n}",
